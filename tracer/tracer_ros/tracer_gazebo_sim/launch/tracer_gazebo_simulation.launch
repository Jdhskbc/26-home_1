<launch>

    <!-- 将 Urdf 文件的内容加载到参数服务器 -->
    <param name="robot_description" command="$(find xacro)/xacro $(find tcurdf)/urdf/tcurdf.xacro" />
    <!-- 启动 gazebo -->
    <include file="$(find gazebo_ros)/launch/empty_world.launch">
        <!-- <arg name="world_name" value="/home/ge/catkin_src/src/wpr_simulation/worlds/robocup2023.world"/> -->
        <!-- <arg name="world_name" value="/home/h/robocup_map/gazebo/models/robocup2023.world"/> -->
        <arg name="paused" value="true"/>
        <arg name="use_sim_time" value="true"/>
        <arg name="gui" value="true"/>
        <arg name="headless" value="false"/>
        <arg name="debug" value="false"/>
        <arg name="recording" value="false"/>
    </include>

    <!-- <include file="/home/jt001/tracer_ws/src/ur_ros/universal_robot-noetic-devel/ur_gazebo/launch/ur3_bringup.launch" /> -->

    <!-- PID 参数
